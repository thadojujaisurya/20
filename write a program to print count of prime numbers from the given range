import math
mini,maxi=map(int,input().split())
print(mini,maxi)
n=10000001
seive=[True]*n
seive[0]=seive[1]=False
for i in range(2,int(math.sqrt(n))+1):
    if seive[i]:
        for j in range(i*i,n,i):
            seive[j]=False
count=0
for i in range(mini,maxi+1):
    if seive[i]:
        count+=1
print(count)
